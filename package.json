  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^12.20.15",
    "eslint": "8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^6.0.0",
